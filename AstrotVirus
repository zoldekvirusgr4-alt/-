local function TeleportTo(x,y,z)
	local lp = game.Players.LocalPlayer
	local c = lp.Character
	if c and c:FindFirstChild("HumanoidRootPart") then
		c.HumanoidRootPart.CFrame = CFrame.new(x,y,z)
	end
end

