local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local SelectedPlayer

local function FindByPartial(text)
	text = text:lower()
	for _,p in ipairs(Players:GetPlayers()) do
		if p.Name:lower():sub(1,#text) == text then
			return p
		end
	end
end

Tab_Taf3eel:AddTextbox({
	Name="Ø§Ø³Ù… Ø§Ù„Ù„Ø§Ø¹Ø¨",
	TextDisappear=true,
	Callback=function(v)
		local p = FindByPartial(v)
		if p then
			SelectedPlayer = p
			OrionLib:MakeNotification({Name="ØªÙ… Ø§Ù„ØªØ­Ø¯ÙŠØ¯",Content=p.Name,Time=2})
		end
	end
})

----------------------------------------------------------------
-- ESP Toggle
----------------------------------------------------------------
local ESP
Tab_Taf3eel:AddToggle({
	Name="ESP",
	Default=false,
	Callback=function(on)
		if not SelectedPlayer or not SelectedPlayer.Character then return end
		if on then
			ESP = Instance.new("Highlight")
			ESP.FillColor = Color3.fromRGB(255,0,0)
			ESP.OutlineColor = Color3.fromRGB(255,255,255)
			ESP.FillTransparency = 0.5
			ESP.Adornee = SelectedPlayer.Character
			ESP.Parent = game.CoreGui
		else
			if ESP then ESP:Destroy() ESP=nil end
		end
	end
})

----------------------------------------------------------------
-- View Toggle
----------------------------------------------------------------
local Cam = workspace.CurrentCamera
local OldSubject
Tab_Taf3eel:AddToggle({
	Name="View",
	Default=false,
	Callback=function(on)
		if not SelectedPlayer or not SelectedPlayer.Character then return end
		local hum = SelectedPlayer.Character:FindFirstChildOfClass("Humanoid")
		if not hum then return end
		if on then
			OldSubject = Cam.CameraSubject
			Cam.CameraSubject = hum
		else
			Cam.CameraSubject = OldSubject
		end
	end
})

----------------------------------------------------------------
-- Teleport Button
----------------------------------------------------------------
Tab_Taf3eel:AddButton({
	Name="Teleport",
	Callback=function()
		local lp = Players.LocalPlayer
		if SelectedPlayer and SelectedPlayer.Character and lp.Character then
			local hrp = SelectedPlayer.Character:FindFirstChild("HumanoidRootPart")
			if hrp then
				lp.Character.HumanoidRootPart.CFrame = hrp.CFrame * CFrame.new(0,0,-3)
			end
		end
	end
})

----------------------------------------------------------------
-- AST Toggle (Ù…Ø­Ù„ÙŠ ÙÙ‚Ø·)
----------------------------------------------------------------
local ASTConn
Tab_Taf3eel:AddToggle({
	Name="AST",
	Default=false,
	Callback=function(on)
		local lp = Players.LocalPlayer
		local char = lp.Character
		if ASTConn then
			ASTConn:Disconnect()
			ASTConn = nil
			if char and char:FindFirstChild("Humanoid") then
				char.Humanoid.PlatformStand = false
			end
		end
		if not on then return end
		if not SelectedPlayer or not SelectedPlayer.Character or not char then return end

		local lHRP = char:WaitForChild("HumanoidRootPart")
		local lHum = char:WaitForChild("Humanoid")
		local tHRP = SelectedPlayer.Character:WaitForChild("HumanoidRootPart")

		lHum.PlatformStand = true -- Ø¨Ø¯ÙˆÙ† Ø¬Ø§Ø°Ø¨ÙŠØ©

		local speed = 30      -- Ø³Ø±Ø¹Ø© Ø£Ø¹Ù„Ù‰
		local amplitude = 6   -- Ù…Ø¯Ù‰ Ø§Ù„Ø­Ø±ÙƒØ©

		ASTConn = RunService.RenderStepped:Connect(function(dt)
			if not lHRP or not tHRP then return end
			local look = tHRP.CFrame.LookVector
			local move = math.sin(tick()*speed) * amplitude * dt
			local pos = lHRP.Position + Vector3.new(look.X,0,look.Z) * move
			lHRP.CFrame = CFrame.new(pos, pos + Vector3.new(look.X,0,look.Z))
		end)
	end
})

